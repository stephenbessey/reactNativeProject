{
  "name": "workout-partner-app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "test:once": "jest",
    "test:coverage": "jest --coverage",
    "lint": "expo lint"
  },
  "jest": {
    "preset": "jest-expo",
    "testMatch": [
      "**/__tests__/**/*.(ts|tsx|js)",
      "**/*.(test|spec).(ts|tsx|js)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "testEnvironment": "jsdom"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "expo": "~53.0.16",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.1",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.2",
    "expo-screen-orientation": "~8.1.7",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.0",
    "react-test-renderer": "19.0.0",
    "typescript": "^5.8.3"
  },
  "private": true
}
